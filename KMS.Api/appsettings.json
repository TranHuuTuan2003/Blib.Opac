{
    "Serilog": {
        "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.File"],
        "MinimumLevel": {
            "Default": "Error"
        },
        "WriteTo": [
            { "Name": "Console" },
            {
                "Name": "File",
                "Args": {
                    "path": "Logs/service.log",
                    "rollingInterval": "Day",
                    "retainedFileCountLimit": 7,
                    "fileSizeLimitBytes": 10485760
                }
            },
            {
                "Name": "Logger",
                "Args": {
                    "filter": {
                        "Name": "ByIncludingOnly",
                        "Args": {
                            "expression": "Contains(@Message, '[QUERY]')"
                        }
                    },
                    "configureLogger": {
                        "WriteTo": [
                            {
                                "Name": "File",
                                "Args": {
                                    "path": "Logs/query.log",
                                    "rollingInterval": "Day",
                                    "retainedFileCountLimit": 7,
                                    "fileSizeLimitBytes": 10485760
                                }
                            }
                        ]
                    }
                }
            }
        ],
        "Filter": [
            {
                "Name": "ByExcluding",
                "Args": {
                    "expression": "Contains(@Message, '/img/') or Contains(@Message, '/css/') or Contains(@Message, '/js/') or Contains(@Message, '/favicon.ico')"
                }
            }
        ],
        "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"],
        "Properties": {
            "Application": "ApplicationName"
        }
    },
